# GInsert the following line at the top of your openssl.cnf file
openssl_conf = openssl_init

# Insert the following line at the bottom of your openssl.cnf file
[openssl_init]
engines      = engine_section

[engine_section]
pkcs11       = pkcs11_section

[pkcs11_section]
engine_id    = engine_pkcs11
dynamic_path = engine_pkcs11
MODULE_PATH  = libaaa_pkcs11.dll
init = 0
